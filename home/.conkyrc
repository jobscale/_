conky.config = {
    color1 = 'light gray',
    color2 = 'gray',
    color3 = 'dim gray',
    color4 = '#eacaba',
    color5 = '#424222',
    template0 = '${if_mounted \\1}\n$color2 \\3  $color1${fs_used \\1}/${fs_size \\1}  $color4${fs_bar 8 \\1}$color3\n${diskiograph_read \\2 18,160 07CC0D fc8060 -t}${alignr} ${diskiograph_write \\2 18,160 07CC0D fc8060 -t}$endif',
    template1 = '${if_up \\1}\n${voffset -20}$color2 $alignr \\1  $color1${addrs \\1}$color3  \n  Up: $color1${upspeed \\1}${alignr}$color3${upspeedgraph \\1 18,220 212121 00aaaa}$color3\n  Down: $color1${downspeed \\1}$alignr$color3${downspeedgraph \\1 18,220 212121 00aa00}$color3$endif',
    template2 = '${cpugraph \\1 18,160 212121 f08000}${alignr} ${cpugraph \\2 18,160 212121 f08000}',
    alignment = 'top_right',
    gap_x = 0,
    gap_y = 0,
    background = false,
    border_width = 1,
    cpu_avg_samples = 2,
    default_color = 'white',
    default_outline_color = 'white',
    default_shade_color = 'white',
    draw_borders = false,
    draw_graph_borders = true,
    draw_outline = false,
    draw_shades = false,
    double_buffer = true,
    override_utf8_locale = true,
    use_xft = true,
    font = 'IPAGothic:style=Regular:size=10',
    minimum_height = 5,
    minimum_width = 5,
    net_avg_samples = 2,
    no_buffers = true,
    out_to_console = false,
    out_to_stderr = false,
    extra_newline = false,
    own_window = true,
    own_window_class = 'Conky',
    own_window_type = 'normal',
    own_window_hints = 'undecorated,below,sticky,skip_taskbar,skip_pager',
    own_window_transparent = true,
    own_window_argb_visual = true,
    stippled_borders = 0,
    update_interval = 1.0,
    uppercase = false,
    use_spacer = 'none',
    show_graph_scale = false,
    show_graph_range = false
}

conky.text = [[
${color3}Load: $color1$loadavg$color3${alignr}Uptime: $color1$uptime$color3
RAM: $color1$memperc%$alignc$mem/$memmax$alignr$color3 Freq: $color1${freq_g}GHz
$color4${membar 8}$color3
\
CPU: $color1$cpu%  ${color3}Swap: $color1$swapperc%  $color4${swapbar 8}$color3
${template2 cpu1 cpu2}
${template2 cpu3 cpu4}
${if_match "$nodename" == "bookworm"}\
${template2 cpu5 cpu6}
$endif\
${if_match "$nodename" == "jammy"}\
${template2 cpu5 cpu6}
${template2 cpu7 cpu8}
${template2 cpu9 cpu10}
${template2 cpu11 cpu12}
${template2 cpu13 cpu14}
${template2 cpu15 cpu16}
$endif\
${color5}Network         $stippled_hr\
${template1 enp4s0}\
${template1 ens9}\
${template1 wlp3s0}\
${template1 enp0s3}\
${template1 wlan0}\
${template1 eth0}
${color5}Storage         $stippled_hr \
${template0 /               sda SSD}\
${template0 /home/vbox      sdb SSD}\
${template0 /home/storage   sdd SSD}\
${template0 /home/pinky     sdc SSD}\
\
${template0 /home           sdb SS}\
${template0 /usr            sdc SS}\
${template0 /task           sdd SS}\
${template0 /var/lib        sde SS}\
\
${template0 /media/jobscale/d9fb763b-4004-4d37-8982-d05faef4ad5a sdd SSD}\
${template0 /media/jobscale/b1ed86b0-511e-4eaa-85d8-5c145ae8af44 sde SD}\
${template0 /media/jobscale/7b2fc0f8-0a3b-47b5-9dc4-75075f935c09 sdd SSD}\
${template0 /media/jobscale/e657358c-0471-4cb6-87db-a24a055f2031 sdf SD}\
${template0 /media/jobscale/8f387411-a7dd-4144-ae04-7aa7075b275c sdb ext4}\
${template0 /media/jobscale/Central                              sdc Central}\

${color5}Date         $stippled_hr
${voffset 7}${alignr}$color2${time %B %eæ—¥ %A} ${voffset -17} $color3 ${font Regular:size=26} ${time %H:%M:%S}${font} ${alignr}
${voffset -4}${color5}Process         $stippled_hr $color3
${alignc}Proces: $color1$processes$color3        Thrds: $color1$threads$color3        Runs: $color1$running_processes
$alignr PID   CPU%   MEM%$color2
  ${top name 1}$alignr${top pid 1} ${top cpu 1}% ${top mem 1}%
  ${top name 2}$alignr${top pid 2} ${top cpu 2}% ${top mem 2}%
  ${top name 3}$alignr${top pid 3} ${top cpu 3}% ${top mem 3}%
  ${top name 4}$alignr${top pid 4} ${top cpu 4}% ${top mem 4}%
]]