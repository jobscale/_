conky.config = {
    color1 = 'light gray',
    color2 = '#8090a0',
    color3 = '#303030',
    color4 = '#a09060',
    color5 = 'dim gray',
    color6 = '#cc4488',
    template0 = '${if_mounted \\1}\n$color2 \\3  ${color1}${alignr}${fs_size \\1} ${voffset 2} ${color4}${alignr}${fs_bar 8,220 \\1}$color3\n${diskiograph_read \\2 18,160 07CC0D fc8060 -t}${alignr} ${diskiograph_write \\2 18,160 07CC0D fc8060 -t}$endif',
    template1 = '${if_match "${execi 60 ip a | grep -v altname | grep -A 2 \\1: | grep "inet" | wc -l}" != "0"}$color2 $alignr \\1  ${color1}${addrs \\1}$color2  \n Up ${color1}${alignr}${upspeed \\1}  ${alignr}$color3${upspeedgraph \\1 18,220 212121 00aaaa}$color2\n Down ${color1}${alignr}${downspeed \\1}  $alignr$color3${downspeedgraph \\1 18,220 212121 00aa00}\n$color3$endif',
    template2 = '${color3}${cpugraph \\1 18,160 212121 f08000}${alignr} ${cpugraph \\2 18,160 212121 f08000}',
    alignment = 'top_right',
    gap_x = 0,
    gap_y = 0,
    background = false,
    border_width = 1,
    cpu_avg_samples = 2,
    default_color = 'white',
    default_outline_color = 'white',
    default_shade_color = 'white',
    draw_borders = false,
    draw_graph_borders = true,
    draw_outline = false,
    draw_shades = false,
    double_buffer = true,
    override_utf8_locale = true,
    use_xft = true,
    font = 'Regular:size=10',
    minimum_height = 5,
    minimum_width = 5,
    net_avg_samples = 2,
    no_buffers = true,
    out_to_console = false,
    out_to_stderr = false,
    extra_newline = false,
    own_window = true,
    own_window_class = 'Conky',
    own_window_type = 'normal',
    own_window_hints = 'undecorated,below,sticky,skip_taskbar,skip_pager',
    own_window_transparent = true,
    own_window_argb_visual = true,
    stippled_borders = 0,
    update_interval = 1.0,
    uppercase = false,
    use_spacer = 'none',
    show_graph_scale = false,
    show_graph_range = false
}

conky.text = [[
${voffset -3}${color2}Load   $color1$loadavg ${alignr}${color2}Uptime   $color1$uptime
${voffset  0}${color2}DDR ${alignr}${color1}$memperc% $memmax ${voffset 2} ${alignr}${color4}${membar 8,200}
${voffset -3}${color2}Swap ${alignr}${color1}$swapperc% $swapmax ${voffset 2} ${alignr}${color4}${swapbar 8,200}
${voffset -3}${color2}CPU     ${color1}$cpu%    \
${color6}    ${execi 10 "head -1 $HOME/.work/dark-cpu | awk '{print $1}'"}   ${alignr}  ${color2}Freq   $color1${freq_g}GHz
${voffset -3}${alignr} \
${color2}   NVMe${color6}  ${execi 10 "head -1 $HOME/.work/dark-cpu | awk '{print $2}'"} \
${color2}   GPU${color6}  ${execi 10 "head -1 $HOME/.work/dark-cpu | awk '{print $4}'"} \
${color2}   WiFi${color6}  ${execi 10 "head -1 $HOME/.work/dark-cpu | awk '{print $3}'"}
${voffset  6}\
${template2 cpu1 cpu2}
${if_match "${exec printf "%02d" $(nproc --all)}" >= "04"}\
${template2 cpu3 cpu4}
$endif\
${if_match "${exec printf "%02d" $(nproc --all)}" >= "06"}\
${template2 cpu5 cpu6}
$endif\
${if_match "${exec printf "%02d" $(nproc --all)}" >= "08"}\
${template2 cpu7 cpu8}
$endif\
${if_match "${exec printf "%02d" $(nproc --all)}" >= "10"}\
${template2 cpu9 cpu10}
$endif\
${if_match "${exec printf "%02d" $(nproc --all)}" >= "12"}\
${template2 cpu11 cpu12}
$endif\
${if_match "${exec printf "%02d" $(nproc --all)}" >= "14"}\
${template2 cpu13 cpu14}
$endif\
${if_match "${exec printf "%02d" $(nproc --all)}" >= "16"}\
${template2 cpu15 cpu16}
$endif\
\
${color5}Network         $stippled_hr
${voffset -5}\
${template1 wlp2s0}\
${template1 wlp3s0}\
${template1 eno1}\
${template1 enp4s0}\
${template1 ens9}\
${template1 enp0s3}\
${template1 wlan0}\
${template1 eth0}\
\
${color5}Storage         $stippled_hr \
${template0 / nvme0n1p2 nvme}\
${template0 /media/jobscale/bf45adae-828d-45bc-850e-b1f57aba80cf sda1 sda1}\

${color5}Date         $stippled_hr
${voffset  0}${color2}      ${time %A  %e  %B}
${voffset  0}${color1}${alignc}${exec TZ=Europe/Paris date +'%H:%M:%S'} ${voffset -20}${alignr}${color5}${font Regular:size=22}${time %H:%M:%S}${font}
${voffset -4}${color5}Process         $stippled_hr
${alignc}${color2}Proces   $color1$processes$color2        Thread   $color1$threads$color2        Running   $color1$running_processes
${alignr}${color2}PID   CPU%   MEM%$color5
  ${top name 1}$alignr${top pid 1} ${top cpu 1}% ${top mem 1}%
  ${top name 2}$alignr${top pid 2} ${top cpu 2}% ${top mem 2}%
  ${top name 3}$alignr${top pid 3} ${top cpu 3}% ${top mem 3}%
  ${top name 4}$alignr${top pid 4} ${top cpu 4}% ${top mem 4}%
  ${top name 5}$alignr${top pid 5} ${top cpu 5}% ${top mem 5}%
\
${voffset  6}\
${if_match "$nodename" == "dark"}\
${color2}n100 ${alignr}${color6}${exec "head -1 $HOME/.work/n100-cpu | awk '{print $1}'"}   ${color5}${execgraph "head -1 $HOME/.work/n100-cpu | awk '{print $2}'" 15,220 212121 80a000}
${color2} ${alignr}${color6}${exec "head -1 $HOME/.work/n100-cpu | awk '{print $3}'"}   ${color5}${execgraph "head -1 $HOME/.work/n100-cpu | awk '{print $4}'" 15,220 212121 80a000}
${color2}mac ${alignr}${color6}${exec "head -1 $HOME/.work/mac-cpu | awk '{print $1}'"}   ${color5}${execgraph "head -1 $HOME/.work/mac-cpu | awk '{print $2}'" 15,220 212121 80a000}
${color2}${alignr}${color2}recv  ${color1}${exec "head -1 $HOME/.work/mac-cpu | awk '{print $3}'"}   ${color2}send  ${color1}${exec "head -1 $HOME/.work/mac-cpu | awk '{print $4}'"}
$endif\
]]
